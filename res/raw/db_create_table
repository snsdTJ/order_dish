CREATE TABLE T_DATA_DICTIONARY (ID INTEGER PRIMARY KEY AUTOINCREMENT, CODE VARCHAR(50) NOT NULL,NAME VARCHAR(50) NOT NULL, TYPE INTEGER NOT NULL)
CREATE TABLE T_DATA_DICTIONARY_TYPE (ID INTEGER PRIMARY KEY AUTOINCREMENT, NAME VARCHAR(100) NOT NULL)
CREATE TABLE T_DISH_INFO (ID INTEGER PRIMARY KEY AUTOINCREMENT, NAME VARCHAR(100) NOT NULL, DISH_TYPE INTEGER NOT NULL, PRICE FLOAT NOT NULL, CURRENCY_TYPE INTEGER, UNIT INTEGER NOT NULL, PHOTO VARCHAR(200) NOT NULL)
CREATE TABLE T_EMPLOYEE (ID INTEGER PRIMARY KEY AUTOINCREMENT, CODE CHAR(6) NOT NULL, PWD VARCHAR(20) NOT NULL, USERNAME VARCHAR(100) NOT NULL, ROLE_ID INTEGER NOT NULL)
CREATE TABLE T_ORDER (ID INTEGER PRIMARY KEY AUTOINCREMENT, TABLE_INFO_ID INTEGER NOT NULL, WAITER_ID INTEGER NOT NULL, CASHIER_ID INTEGER, BEGIN_DATETIME DATETIME NOT NULL, END_DATETIME DATETIME, TOTAL_MONEY FLOAT, ISPAY INTEGER NOT NULL)
CREATE TABLE T_ORDER_DETAIL (ID INTEGER PRIMARY KEY AUTOINCREMENT, ORDER_ID INTEGER NOT NULL, DISH_INFO_ID INTEGER NOT NULL, CURRENT_PRICE FLOAT NOT NULL, DISH_NUMBER INTEGER NOT NULL)
CREATE TABLE T_ROLE (ID INTEGER PRIMARY KEY AUTOINCREMENT,NAME VARCHAR(100) NOT NULL)
CREATE TABLE T_TABLE_INFO (ID INTEGER PRIMARY KEY AUTOINCREMENT, NO INTEGER NOT NULL, SIZE_ID INTEGER NOT NULL, STATE INTEGER NOT NULL, DESCRIBE VARCHAR(200))
CREATE TABLE T_TABLE_SIZE (ID INTEGER PRIMARY KEY AUTOINCREMENT, SIZE INTEGER NOT NULL)